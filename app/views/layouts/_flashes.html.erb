    <div class="container">
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type.empty? ? 'info' : message_type %> alert-dismissible fade in">
        <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
        <%= message %>
        </div>
        <% end %>
    </div>