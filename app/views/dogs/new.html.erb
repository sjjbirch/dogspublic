<h1>Dogs#new</h1>
<p>new dog thing</p>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_with(model: @dog, local: true) do |f| %>
        
            <%= f.label :rname, "Real Name" %>
            <%= f.text_field :rname, class: 'form-control' %>

            <%= f.label :cname, "Call Name" %>
            <%= f.text_field :cname, class: 'form-control' %>

            <%= f.label :dob, "DOB" %>
            <%= f.date_field :dob, class: 'form-control' %>

            <%= f.label :owner, "Owner" %>
            <%= f.text_field :owner, class: 'form-control' %>

            <%= f.label :handler, "Handler" %>
            <%= f.text_field :handler, class: 'form-control' %>

            <%= f.label :sex, "Sex" %>
            <%= f.select :sex, options_for_select([['N'], ['F'], ['M']]) %> 

            <%= f.submit "Create dog", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>

