<div class="container">

    <h1><%= @dog.rname %></h1>

<%= link_to "Back to my dogs", dogs_path %>

    <table class="table">
        <thead>
            <tr>
                <th>Real Name</th>
                <th>Call Name</th>
                <th>DOB</th>
                <th>Breeder</th>
                <th>Handler</th>
                <th>Sex</th>
            </tr>
        </thead>

        <tbody>
            <tr>
                <td><%= @dog.rname %></td>
                <td><%= @dog.cname %></td>
                <td><%= @dog.dob %></td>
                <td><%= @dog.breeder %></td>
                <td><%= @dog.handler %></td>
                <td><%= @dog.sex %></td>
            </tr>
        </tbody>
    </table>
</div>